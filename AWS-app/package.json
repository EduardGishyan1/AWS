    "babel-loader": "9.1.3",
    "json": "^11.0.0",
    "pino": "8.0.0",
    "pip": "^0.0.1",
    "regenerator-runtime": "0.14.0",
    "serverless-online": "^2.2.0",
    "serverless-s3-sync": "^3.4.0",
    "webpack": "5.88.2"
  },
  "devDependencies": {
    "@babel/preset-env": "7.22.9",
    "@types/jest": "29.5.2",
